 <div class="pc-container">
        <div class="pc-content">
            <!-- [ breadcrumb ] start -->
            <div class="page-header">
                <div class="page-block">
                    <div class="row align-items-center">
                        <div class="col-md-12">
                            <div class="page-header-title">
                                <h5 class="m-b-10">@ViewData["Title"]</h5>
                            </div>
                            <br />
                            <br />
                            <ul class="breadcrumb">
                                <li class="breadcrumb-item"><a href="../dashboard/index.html">Home</a></li>
                                <li class="breadcrumb-item"><a href="javascript: void(0)">@ViewData["Title"]</a></li>
                                <li class="breadcrumb-item" aria-current="page">Home</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <!-- [ breadcrumb ] end -->
            <!-- [ Main Content ] start -->
            @RenderBody()
        </div>
    </div>
 <!-- [ Main Content ] end -->
@await Component.InvokeAsync("_AdminLayoutFooterComponent")

@await Component.InvokeAsync("_AdminLayoutScriptComponent")  
</body>

</html>